{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch Main App",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/main.go",
            "env": {},
            "args": []
        },
        {
            "name": "Launch Go File",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${file}",
            "env": {},
            "args": [],
            "output": "${workspaceFolder}/.vscode/.debugbin",
            "internalConsoleOptions": "openOnSessionStart"
        }
    ]
}
